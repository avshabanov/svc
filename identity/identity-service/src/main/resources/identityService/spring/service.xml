<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <import resource="classpath:/spring/default-service-base.xml"/>
  <import resource="security.xml"/>

  <!-- Message Converters (used in security and webmvc configurations) -->
  <bean id="protobufHttpMessageConverter" class="com.truward.brikar.protobuf.http.ProtobufHttpMessageConverter" />
  <bean id="protobufJsonHttpMessageConverter" class="com.truward.brikar.protobuf.http.json.ProtobufJsonHttpMessageConverter" />

  <!-- Local bean definitions -->
  <bean class="com.alexshabanov.services.identity.model.error.IdentityRestErrors" />
</beans>
